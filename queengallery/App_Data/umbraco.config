      <BlogPostRepository id="1122" key="017db379-91df-4747-9b01-04b29c921547" parentID="1107" level="3" creatorID="0" sortOrder="5" createDate="2016-11-02T15:19:15" updateDate="2016-11-03T15:17:00" nodeName="EXHIBITION" urlName="exhibition" path="-1,1100,1107,1122" isDoc="" nodeType="1059" creatorName="admin" writerName="admin" writerID="0" template="1054" nodeTypeAlias="BlogPostRepository">
